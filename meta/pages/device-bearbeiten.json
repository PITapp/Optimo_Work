                          "type": "col"
                        }
                      ],
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "type": "row"
                    },
                    {
                      "components": [
                        {
                          "class": "col-md-3",
                          "components": [
                            {
                              "component": "Info",
                              "name": "InfoLabel",
                              "style": {
                                "width": "100%"
                              },
                              "text": "Info",
                              "type": "label"
                            }
                          ],
                          "type": "col"
                        },
                        {
                          "class": "col-md-9",
                          "components": [
                            {
                              "name": "Info",
                              "rows": 7,
                              "style": {
                                "width": "100%"
                              },
                              "type": "textarea",
                              "value": "${dsoDevice.Info}"
                            }
                          ],
                          "type": "col"
                        }
                      ],
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "type": "row"
                    },
                    {
                      "components": [
                        {
                          "class": "col offset-sm-3",
                          "components": [
                            {
                              "buttonStyle": "success",
                              "buttonType": "submit",
                              "icon": "done",
                              "name": "button0",
                              "text": "Speichern",
                              "type": "button"
                            },
                            {
                              "buttonStyle": "light",
                              "events": {
                                "click": [
                                  {
                                    "type": "back"
                                  }
                                ]
                              },
                              "name": "button1",
                              "style": {
                                "marginLeft": "1rem"
                              },
                              "text": "Abbruch",
                              "type": "button"
                            }
                          ],
                          "name": "col0",
                          "type": "col"
                        }
                      ],
                      "name": "row0",
                      "style": {
                        "textAlign": "right"
                      },
                      "type": "row"
                    }
                  ],
                  "events": {
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Gerät konnte nicht gespeichert werden!",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "dbOptimo.updateInventurDevice",
                        "parameters": [
                          {
                            "name": "DeviceID",
                            "value": "${parameters.DeviceID}"
                          },
                          {
                            "name": "InventurDevice",
                            "value": "${event}"
                          }
                        ],
                        "then": [
                          {
                            "detail": "Gerät gespeichert",
                            "severity": "success",
                            "type": "notify"
                          },
                          {
                            "type": "close-popup"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "name": "form0",
                  "type": "template-form"
                }
              ],
              "type": "col"
            }
          ],
          "type": "row"
        }
      ],
      "container": "main",
      "type": "content"
    }
  ],
  "events": {
    "load": [
      {
        "error": [],
        "name": "dbOptimo.getInventurDeviceByDeviceID",
        "parameters": [
          {
            "name": "DeviceID",
            "value": "${parameters.DeviceID}"
          }
        ],
        "then": [
          {
            "name": "dsoDevice",
            "type": "set",
            "value": "${result}"
          }
        ],
        "type": "invoke"
      }
    ]
  },
  "layout": "Main",
  "name": "DeviceBearbeiten"
}