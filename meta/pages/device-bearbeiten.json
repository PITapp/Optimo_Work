                              "locale": "de",
                              "name": "RegistriertAm",
                              "placeholder": "",
                              "showTime": true,
                              "style": {
                                "width": "100%"
                              },
                              "type": "datepicker",
                              "value": "${dsoDevice.RegistriertAm}"
                            }
                          ],
                          "type": "col"
                        }
                      ],
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "type": "row"
                    },
                    {
                      "components": [
                        {
                          "class": "col-md-3",
                          "components": [
                            {
                              "component": "AnmeldungAm",
                              "name": "AnmeldungAmLabel",
                              "style": {
                                "width": "100%"
                              },
                              "text": "Anmeldung am",
                              "type": "label"
                            }
                          ],
                          "type": "col"
                        },
                        {
                          "class": "col-md-9",
                          "components": [
                            {
                              "dateFormat": "dd.mm.yy",
                              "locale": "de",
                              "name": "AnmeldungAm",
                              "showTime": true,
                              "style": {
                                "width": "100%"
                              },
                              "type": "datepicker",
                              "value": "${dsoDevice.AnmeldungAm}"
