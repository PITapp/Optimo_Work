{
  "access": [
    "Authenticated"
  ],
  "components": [
    {
      "components": [
        {
          "components": [
            {
              "class": "col-md-12",
              "components": [
                {
                  "components": [
                    {
                      "components": [
                        {
                          "class": "col-md-3",
                          "components": [
                            {
                              "component": "DeviceNummer",
                              "name": "DeviceNummerLabel",
                              "style": {
                                "width": "100%"
                              },
                              "text": "Gerätenummer",
                              "type": "label"
                            }
                          ],
                          "type": "col"
                        },
                        {
                          "class": "col-md-9",
                          "components": [
                            {
                              "maxLength": 30,
                              "name": "DeviceNummer",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "type": "textbox",
                              "value": "${dsoDevice.DeviceNummer}"
                            },
                            {
                              "component": "DeviceNummer",
                              "name": "DeviceNummerRequiredValidator",
                              "style": {
                                "position": "absolute"
                              },
                              "text": "DeviceNummer is required",
                              "type": "required-validator"
                            }
                          ],
                          "type": "col"
                        }
                      ],
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "type": "row"
                    },
                    {
                      "components": [
                        {
                          "class": "col-md-3",
                          "components": [
                            {
                              "component": "DeviceTitel",
                              "name": "DeviceTitelLabel",
                              "style": {
                                "width": "100%"
                              },
                              "text": "Gerätetitel",
                              "type": "label"
                            }
                          ],
                          "type": "col"
                        },
                        {
                          "class": "col-md-9",
                          "components": [
                            {
                              "maxLength": 150,
                              "name": "DeviceTitel",
                              "style": {
                                "display": "block",
                                "width": "100%"
                              },
                              "type": "textbox",
                              "value": "${dsoDevice.DeviceTitel}"
                            },
                            {
                              "component": "DeviceTitel",
                              "name": "DeviceTitelRequiredValidator",
                              "style": {
                                "position": "absolute"
                              },
                              "text": "DeviceTitel is required",
                              "type": "required-validator"
                            }
                          ],
                          "type": "col"
                        }
                      ],
                      "style": {
                        "marginBottom": "1rem"
                      },
                      "type": "row"
                    },
                    {
                      "components": [
                        {
                          "class": "col-md-3",
                          "components": [
                            {
                              "component": "RegistriertAm",
